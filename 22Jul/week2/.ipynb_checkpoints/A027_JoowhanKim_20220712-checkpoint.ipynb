   "source": [
    "#숫자 하나씩 stack에 넣으면서, 넣는 숫자가 기존에 있는 숫자들보다 크다면 기존의 숫자를 뺀다.\n",
    "number = \"4177252841\"\n",
    "k = 4\n",
    "stk = []\n",
    "for i in number:\n",
    "    while stk and stk[-1] < i and k>0:\n",
    "        k-=1\n",
    "        stk.pop()\n",
    "    stk.append(i)\n",
    "    print(\"K=\",k,\"i=\" , i, stk)\n",
    "\"\".join(stk[:len(stk)-k])"
   ]
