    "digits = [4,3,2,1]\n",
    "a = int(len(digits))\n",
    "num = 0\n",
    "for i in range(a):\n",
    "    num +=digits[i] * (10**(a-i-1))\n",
    "num = num+1\n",
    "num_str = str(num)\n",
    "result = list(map(int, num_str))"
