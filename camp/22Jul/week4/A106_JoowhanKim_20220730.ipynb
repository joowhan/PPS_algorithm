   "source": [
    "n = int(input())\n",
    "element = 0\n",
    "line =0\n",
    "while n > element:\n",
    "    line +=1\n",
    "    element += line\n",
    "diff = element - n\n",
    "if line%2 == 0:\n",
    "    top = line - diff\n",
    "    bottom = diff + 1\n",
    "else:\n",
    "    top = diff + 1\n",
    "    bottom = line - diff\n",
    "\n",
    "print(\"%d/%d\"%(top,bottom))"
   ]
  },
