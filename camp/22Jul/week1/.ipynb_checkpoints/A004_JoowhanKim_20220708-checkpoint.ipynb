    "def solution(arr, divisor):\n",
    "    answer = []\n",
    "    count = 0\n",
    "    arr.sort()\n",
    "    for i in range(len(arr)):\n",
    "        if arr[i]%divisor ==0:\n",
    "            answer.append(arr[i])\n",
    "            count = count+1\n",
    "    if count <=0:\n",
    "        answer.append(-1)\n",
    "    return answer"
   ]
